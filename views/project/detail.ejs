<div class="container-fluid">
	 <% if (typeof my !== 'undefined') { %>
  		<div class="panel panel-primary">
    		<div class="panel-heading">My projects</div>
      		<div class="panel-body">
          		<table class="table">

			 		</table>
      		</div>
    	</div>
	<% } else { %>
		<div class="alert alert-info" role="alert">You don't created any projects</div>
	<% } %>
	<hr />
	<% if (typeof public !== 'undefined') { %>
  		<div class="panel panel-primary">
    		<div class="panel-heading">Projects where you are a developer</div>
      		<div class="panel-body">
          		<table class="table">

			 		</table>
      		</div>
    	</div>
	<% } else { %>
		<div class="alert alert-info" role="alert">You are not a developer in any projects</div>
	<% } %>
	<hr />
	<% if (user.admin) { %>
		<% if (typeof other !== 'undefined') { %>
			<div class="panel panel-primary">
				<div class="panel-heading">Other projects</div>
					<div class="panel-body">
						<table class="table">

						</table>
					</div>
			</div>
		<% } else { %>
			<div class="alert alert-info" role="alert">There are not other projects</div>
		<% } %>
	<% } %>
